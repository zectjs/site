<header id="header">
  <div class="outer">
    <div class="inner">
      <h1 id="logo-wrap">
        <a href="<%- url_for(page.lang === 'en' ? '' : page.lang + '/') %>" id="logo">Hexo</a>
        <a id="main-nav-toggle"></a>
      </h1>
      <nav id="main-nav">
        <ul id="main-nav-list">
          <%- header_menu() %>
        </ul>
        <a href="https://github.com/<%- theme.github %>" id="github-btn" title="GitHub" target="_blank"></a>
        <a id="search-btn" class="st-search-launcher" title="<%= __('menu.search') %>"></a>
      </nav>
    </div>
  </div>
</header>